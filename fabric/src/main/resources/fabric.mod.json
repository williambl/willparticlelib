{
    "schemaVersion": 1,
    "id": "willparticlelib",
    "version": "${version}",
    "name": "${mod_name}",
    "description": "Particles !!!",
    "authors": [
        "${mod_author}"
    ],
    "contact": {
        "homepage": "https://fabricmc.net/",
        "sources": "https://github.com/FabricMC/fabric-example-mod"
    },
    "license": "CC0-1.0",
    "icon": "assets/modid/icon.png",
    "environment": "*",
    "entrypoints": {
        "main": [
            "com.williambl.willparticlelib.impl.WillParticleLibFabric"
        ],
        "preLaunch": [
            "com.williambl.willparticlelib.impl.WillParticleLibFabricPrelaunch"
        ]
    },
    "mixins": [
        "willparticlelib.mixins.json",
        "willparticlelib.fabric.mixins.json"
    ],
    "depends": {
        "fabricloader": ">=0.14",
        "fabric": "*",
        "minecraft": "1.20",
        "java": ">=17"
    },
    "suggests": {
        "another-mod": "*"
    }
}
  